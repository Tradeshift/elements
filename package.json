    "@tradeshift/elements.select": "file:packages/components/select",
    "@tradeshift/elements.select-menu": "file:packages/components/select-menu",
    "@tradeshift/elements.spinner": "file:packages/components/spinner",
    "@tradeshift/elements.status": "file:packages/components/status",
